<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.collegeassistant">

    <application
        android:allowBackup="true"
        android:appComponentFactory="whateverString"
        android:icon="@mipmap/applogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:appComponentFactory">
        <activity android:name=".announcement.AnnouncementSenderActivity"></activity>
        <activity android:name=".announcement.AnnouncementActivity" />
        <activity android:name=".attendance.AttendanceActivity" />
        <activity android:name=".table.TableActivity" />
        <activity android:name=".subject.SubjectActivity" />
        <activity android:name=".chat.ChatActivity" />
        <activity android:name=".grades.GradesActivity" />
        <activity android:name=".home.HomeActivity" />
        <activity android:name=".authentication.SignupActivity" />
        <activity android:name=".authentication.LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>